    type='text/css'>
<link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>
<!-- Custom styles for this template -->

{{>navAdmin}}
<div class="container-fluid" id="content" style="margin-top:100px !important ;">
    <div class="row mx-auto w-90 mt-5 pt-5 pl-5">

        {{>verticalMenu}}

        <!-- List Bidder -->
        <div class="col-12 col-md-8 col-xl-9 py-md-3 pl-md-5 bd-content listbid">

            <div id="titleProfileInfo" class="row ">
                <h4 class="col mb-5">LIST BIDDER</h4>
            </div>
            <table class="table table-sm txt-center table-hover" id="listStore">
                <thead>
                    <tr>
                        <th scope="col">USER NAME</th>
                        <th scope="col">PURCHASED </th>
                        <th scope="col">CANCELLATED</th>
                        <th scope="col">POINT</th>
                    </tr>
                </thead>

                <tbody id="productList"></tbody>
                {{#if checkbid}}
                <tr>
                    <h3 style="color: rgb(159, 159, 160);">NO ONE ON LIST</h3>
                </tr>
                {{else}}

                {{#each listbid}}

                <tr>
                    <td>{{this.name}}</td>
                    <td>{{this.purchase}}</td>
                    <td>{{this.cancellate}}</td>
                    <td>{{this.pointbid}}</td>
                </tr>


                {{/each}}
                {{/if}}
                </tbody>

            </table>
            <!-- Pagination -->
            {{!-- {{> pagination}} --}}
        </div>
        <!-- End of list Bidder -->

        <!-- List Seller -->
        <div class="col-12 col-md-8 col-xl-9 py-md-3 pl-md-5 bd-content listsell">

            <div id="titleProfileInfo" class="row ">
                <h4 class="col mb-5">LIST SELLER</h4>
            </div>
            <table class="table table-sm txt-center table-hover" id="listStore">
                <thead>
                    <tr>
                        <th scope="col">USER NAME</th>
                        <th scope="col">PURCHASED </th>
                        <th scope="col">CANCELLATED</th>
                        <th scope="col">POINT BIDDER</th>
                        <th scope="col">POINT SELLER</th>
                        <th scope="col">TOTAL PRODUCT</th>

                    </tr>
                </thead>

                <tbody id="productList">
                    {{#if checksell}}
                    <tr>
                        <h3 style="color: rgb(159, 159, 160);">NO ONE ON LIST</h3>
                    </tr>
                    {{else}}
                    {{#each listsell}}

                    <tr>
                        <td>{{this.name}}</td>
                        <td>{{this.purchase}}</td>
                        <td>{{this.cancellate}}</td>
                        <td>{{this.pointbid}}</td>
                        <td>{{this.pointsell}}</td>
                        {{!-- <td>{{totalproduct}}</td> --}}
                        <td>0</td>
                    </tr>


                    {{/each}}
                    {{/if}}
                </tbody>
            </table>
        </div>
        <!-- End of list seller -->

        <!-- Register become a seller -->
        <div class="col-12 col-md-8 col-xl-9 py-md-3 pl-md-5 bd-content regis">

            <div id="titleProfileInfo" class="row ">
                <h4 class="col mb-5">LIST BIDDER WANT TO REGIST DO SELLER</h4>
            </div>
            <table class="table table-sm txt-center table-hover" id="listStore">
                <thead>
                    <tr>
                        <th scope="col">USER NAME</th>
                        <th scope="col">POINT</th>
                        <th scope="col">CONFIRM</th>
                        <th scope="col">DELETE</th>

                    </tr>
                </thead>

                <tbody id="productList">
                    {{#if checkregist}}
                    <tr>
                        <h3 style="color: rgb(159, 159, 160);">NO ONE ON LIST</h3>
                    </tr>
                    {{else}}
                    {{#each listregist}}

                    <tr>
                        <td>{{this.name}}</td>
                        <td>{{this.point}}</td>
                        <td>
                            <button type="button" class="btn btn-primary" data-toggle="modal"
                                data-target="#exampleModalCenter1">
                                Confirm
                            </button>
                            {{>modalconfirm}}
                        </td>
                        <td>
                            <button type="button" class="btn btn-primary" data-toggle="modal"
                                data-target="#exampleModalCenter">
                                Delete
                            </button>
                            {{>modaldelete}}
                        </td>
                    </tr>


                    {{/each}}
                    {{/if}}
                </tbody>
            </table>
        </div>
        <!-- End of regist -->

    </div>
</div>
</div>
