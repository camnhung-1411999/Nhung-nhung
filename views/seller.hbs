<!-- Custom styles for this template -->

{{>navAdmin}}
<div class="container-fluid" id="content" style="margin-top:100px !important ;">
    <div class="row mx-auto w-90 mt-5 pt-5 pl-5">

        {{>verticalMenu}}


        <!-- List Seller -->
        <div class="col-12 col-md-8 col-xl-9 py-md-3 pl-md-5 bd-content listsell">

            <div id="titleProfileInfo" class="row ">
                <h4 class="col mb-5">LIST SELLER</h4>
            </div>
            <button type="button" class="btn btn-primary" style="float: right!important; margin-bottom:50px;"
                data-toggle="modal" data-target="#ModalCancel">
               Lower level to bidder
            </button>
            {{>modalcancel}}
            <table class="table table-sm txt-center table-hover" id="listStore">
                <thead>
                    <tr>
                        <th scope="col">USER NAME</th>
                        <th scope="col">PURCHASED </th>
                        <th scope="col">CANCELLATED</th>
                        <th scope="col">POINT BIDDER</th>
                        <th scope="col">POINT SELLER</th>
                        <th scope="col">TOTAL PRODUCT</th>

                    </tr>
                </thead>

                <tbody id="productList">
                    {{#if checksell}}
                    <tr>
                        <h3 style="color: rgb(159, 159, 160);">NO ONE ON LIST</h3>
                    </tr>
                    {{else}}
                    {{#each listsell}}

                    <tr>
                        <td>{{this.name}}</td>
                        <td style="text-align: center;">{{this.pluspoint}}</tdstyle="text-align: center;">
                        <td style="text-align: center;">{{this.minuspoint}}</td>
                        <td style="text-align: center;">{{this.pointbid}}</td>
                        <td style="text-align: center;">{{this.rate}}</td>
                        <td style="text-align: center;">{{this.totalproduct}}</td>
                    </tr>


                    {{/each}}
                    {{/if}}
                </tbody>
            </table>
        </div>
        <!-- End of list seller -->


    </div>
</div>
</div>
<script src="/javascripts/agency.min.js"></script>
