{{>navAdmin}}
<div class="container-fluid" id="content" style="margin-top:100px !important ;">
    <div class="row mx-auto w-90 mt-5 pt-5 pl-5">

        <div class="col-2" style="margin-top:80px !important ;margin-right:30px !important ;">
            {{>productMenu}}

        </div>

        <!-- List Bidder -->
        <div class="col-8  bd-content">

            <div id="titleProfileInfo" class="row ">
                <h4 class="col mb-5"style="font-size:30px;color:rgb(159, 159, 160);">LIST PRODUCT</h4>
                <button type="button" class="btn btn-primary" style="float: right!important; margin-bottom:50px;"
                    data-toggle="modal" data-target="#ModalDelete">
                    Delete product
                </button>
                {{>modaldeleteproduct}}
            </div>

            <table class="table table-sm txt-center table-hover" id="listStore">
                <thead>
                    <tr>
                        <th scope="col">NAME PRODUCT</th>
                        <th scope="col"></th>
                        <th scope="col">PRICE </th>
                        <th scope="col">NUMBER OF BIDDING</th>
                        <th scope="col">SELLER</th>
                    </tr>
                </thead>

                <tbody id="productList">
                    {{#each product}}
                    <tr>
                        <td><img src="{{this.image}}" width="50px" height="50px"></td>
                        <td>{{this.ten}}</td>
                        <td>{{this.giamuangay}}</td>
                        <td style="text-align: center;">{{this.soluot}}</td>
                        <td>{{this.user}}</td>
                    </tr>
                    {{/each}}
                </tbody>

            </table>
        </div>

    </div>
</div>
</div>

